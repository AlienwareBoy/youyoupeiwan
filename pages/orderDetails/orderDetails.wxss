.successUser {
  width: 165rpx;
  height: 82rpx;
  background: linear-gradient(180deg, rgba(255, 216, 124, 1) 0%, rgba(255, 176, 70, 1) 100%);
  border-radius: 10rpx;
  position: absolute;
  right: 0rpx;
  bottom: 0rpx;
}

.successUser text {
  font-size: 30rpx;
  color: rgba(63, 75, 85, 1);
}
