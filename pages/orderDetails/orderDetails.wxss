.success text:nth-child(3) {
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(136, 136, 136, 1);
  line-height: 42rpx;
  margin-top: 18rpx;
  margin-bottom: 30rpx;
}

.toPath text {
  font-size: 36rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(63, 75, 85, 1);
