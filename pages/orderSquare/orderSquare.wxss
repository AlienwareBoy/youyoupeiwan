.searchList {
  justify-content: space-around;
  position: relative;
  z-index: 502;
  background: #fff;
  overflow: hidden;
  height: auto;
  /* min-height: 70rpx;
  max-height: 200rpx;
  height: 200rpx; */
  transition: all 0.5s ease;
}

.GradeList {
  flex-wrap: wrap;
}

.maxHeight {
  min-height: 200rpx !important;
  max-height: 600rpx !important;
}

.reset {
  position: absolute;
  right: 50rpx;
  bottom: 30rpx;
}

.reset text:nth-child(1) {
  font-size: 20rpx;
  color: rgba(159, 165, 170, 1);
  line-height: 28rpx;
  margin-right: 10rpx;
}

.minHeight {
  height: 70rpx !important;
}

.Mask {
  width: 100%;
  height: 100%;
  position: absolute;
  background-color: #000;
  z-index: 500;
  top: 0;
  left: 0;
  opacity: 0.5;
}

.priceSearch text {
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 600;
  color: rgba(63, 75, 85, 1);
  line-height: 42rpx;
}

.Interval text:nth-child(2) {
  width: 21rpx;
  height: 2rpx;
  background: rgba(159, 165, 170, 1);
  text-align: center;
  display: inline-block;
  margin: 0 20rpx;
}

.priceSearch .iconfont {
  font-size: 25rpx;
  position: absolute;
  left: 34rpx;
  top: 4rpx;
}

.click {
  color: rgba(63, 75, 85, 1);
}

.noclick {
  color: rgba(63, 75, 85, 0.5) !important;
}

.mixmin {
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 600;
  color: rgba(63, 75, 85, 1);
  line-height: 42rpx;
  margin-left: 10rpx;
}

.bottomBorder {
  width: 70rpx;
  height: 10rpx;
  background: linear-gradient(180deg, rgba(79, 203, 255, 1) 0%, rgba(39, 155, 255, 1) 100%);
  border-radius: 5rpx;
  position: absolute;
  left: 50%;
  bottom: 0;
  transform: translate(-50%, -50%);
}

.priceSearch {
  position: relative;
  height: 70rpx;
  width: 100%;
  text-align: center;
}

.itemList {
  margin: 30rpx 50rpx 0rpx 50rpx;
}

.userBg {
  width: 138rpx;
  height: 138rpx;
  background: rgba(216, 216, 216, 1);
  border-radius: 10rpx;
}

.orderInfo {
  position: relative;
  margin-left: 20rpx;
  align-items: flex-start;
  justify-content: flex-start;
  height: 138rpx;
  width: 300rpx;
}

.userImage {
  width: 30rpx;
  height: 30rpx;
  background: linear-gradient(180deg, rgba(79, 203, 255, 1) 0%, rgba(39, 155, 255, 1) 100%);
  border-radius: 50%;
  display: inline-block;
}

.grade text:nth-child(2) {
  font-size: 20rpx;
  font-family: PingFangSC;
  font-weight: 600;
  color: rgba(63, 75, 85, 1);
  line-height: 28rpx;
  margin-left: 10rpx;
}

.grade {
  position: absolute;
  left: 0rpx;
  bottom: 0rpx;
}

.price {
  position: absolute;
  right: 20rpx;
  bottom: 30rpx;
}

.price text:nth-child(1) {
  font-size: 50rpx;
  font-family: PingFangSC;
  font-weight: 600;
  color: rgba(63, 75, 85, 1);
  line-height: 70rpx;
}

.price text:nth-child(2) {
  font-size: 20rpx;
  font-family: PingFangSC;
  font-weight: 600;
  color: rgba(63, 75, 85, 1);
  line-height: 28rpx;
  display: block;
  text-align: right;
}

.employer {
  font-size: 20rpx;
  font-family: PingFangSC;
  font-weight: 600;
  color: rgba(63, 75, 85, 0.5);
  line-height: 28rpx;
  margin-top: 10rpx;
}

.price text {
  font-size: 50rpx;
  font-family: PingFangSC;
  font-weight: 600;
  color: rgba(63, 75, 85, 1);
  line-height: 70rpx;
}

.userTitle {
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 600;
  color: rgba(63, 75, 85, 1);
  line-height: 42rpx;
}

.item {
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 2px 10px 0px rgba(42, 158, 255, 0.18);
  border-radius: 35rpx;
  padding: 30rpx;
  margin-bottom: 30rpx;
  position: relative;
}

.filterOptions {
  padding-left: 50rpx;
  position: relative;
}

.closeFilter {
  bottom: 899rpx;
}

.priceFilter {
  padding-bottom: 20rpx;
}

.priceFilter .filterTitle {
  font-size: 20rpx;
  font-family: PingFangSC;
  font-weight: 600;
  color: rgba(159, 165, 170, 1);
  line-height: 28rpx;
}

.Interval {
  margin-top: 20rpx;
}

.cominput {
  background: rgba(233, 233, 233, 1);
  border-radius: 10rpx;
  width: 130rpx;
  height: 60rpx;
  text-align: center;
  display: inline-block;
}

.plaClass {
  font-size: 20rpx;
  font-family: PingFangSC;
  font-weight: 600;
  color: rgba(159, 165, 170, 1);
  line-height: 28rpx;
}

.userGame {
  margin-top: 20rpx;
}

.userGame text {
  width: 130rpx;
  height: 60rpx;
  border-radius: 10rpx;
  text-align: center;
  line-height: 60rpx;
  display: inline-block;
  margin-right: 25rpx;
  font-size: 20rpx;
  font-family: PingFangSC;
  font-weight: 600;
}

.userInfo text {
  background: linear-gradient(180deg, rgba(79, 203, 255, 1) 0%, rgba(39, 155, 255, 1) 100%);
  border-radius: 5px;
  display: inline-block;
  padding: 5rpx 10rpx;
  font-size: 18rpx;
  font-family: PingFangSC;
  font-weight: 600;
  color: rgba(255, 255, 255, 1);
  line-height: 25rpx;
  margin-right: 10rpx;
}

.rightSearchPopup {
  width: 579rpx;
  height: 100%;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 5rpx 10rpx 0px rgba(0, 0, 0, 0.2);
  position: absolute;
  right: 0rpx;
  bottom: 0rpx;
  border-top: 1rpx solid #e8e8e8;
  z-index: 502;
}

.isClick {
  background: linear-gradient(180deg, rgba(79, 203, 255, 1) 0%, rgba(39, 155, 255, 1) 100%);
  color: #fff;
}

.noClick {
  background: #e9e9e9;
  color: rgba(159, 165, 170, 1);
}
