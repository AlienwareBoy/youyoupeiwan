.swiper {
  width: 650rpx;
  background: rgba(216, 216, 216, 1);
  border-radius: 35rpx;
}

.index {
  margin: 0 50rpx;
  padding-top:20rpx;
  padding-bottom: 20rpx;
}

.slide-image {
  width: 100%;
  height: 100%;
  border-radius: 35rpx;
}

.announcement {
  align-items: flex-start;
  margin: 20rpx 0px;
}

.announcementList {
  height: 40rpx;
  width: 100%;
  margin-left: 13rpx;
}

.announcementFont {
  font-size: 25rpx;
  font-family: PingFangSC;
  font-weight: 600;
  color: rgba(63, 75, 85, 1);
  line-height: 36rpx;
}

.announcementicon {
  width: 104rpx;
}

.announcementicon text:nth-child(2) {
  font-size: 25rpx;
  font-family: PingFangSC;
  font-weight: 600;
  color: rgba(63, 75, 85, 1);
  line-height: 36rpx;
}

.announcementSwiper {
  height: 50rpx;
}

.Introduction {
  width: 636rpx;
  height: 681rpx;
  background: rgba(216, 216, 216, 1);
  border-radius: 35rpx;
}
.Introduction image{
  width: 100%;
  height: 100%;
}